#include <iostream>
using namespace std;
int main() {
    char c;
    cout<<"Enter an alphabet:"<<endl;
    cin>>c;
    if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u'||c=='A'||c=='E'||c=='O'||c=='U'){
        cout<<"The given alphabet "<<c<<" is a vowel"<<endl;
    }
    else{
        cout<<"The given alphabet "<<c<<" is a consonant"<<endl;
    }

    return 0;
}
